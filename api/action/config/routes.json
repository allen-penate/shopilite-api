{
  "routes": [
    {
      "method": "GET",
      "path": "/actions",
      "handler": "action.find"
    },
    {
      "method": "GET",
      "path": "/actions/count",
      "handler": "action.count"
    },
    {
      "method": "GET",
      "path": "/actions/:id",
      "handler": "action.findOne"
    }
  ]
}
