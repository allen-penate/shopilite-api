{
  "routes": [
    {
      "method": "GET",
      "path": "/carts",
      "handler": "cart.find",
      "config": {
        "policies": []
      }
    },
    {
      "method": "GET",
      "path": "/carts/:id",
      "handler": "cart.findOne",
      "config": {
        "policies": []
      }
    },
    {
      "method": "POST",
      "path": "/carts",
      "handler": "cart.create",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/carts/:code/products/:sku/add",
      "handler": "cart.addProduct",
      "config": {
        "policies": []
      }
    },
    {
      "method": "PUT",
      "path": "/carts/:code/products/:sku/remove",
      "handler": "cart.removeProduct",
      "config": {
        "policies": []
      }
    }
  ]
}
